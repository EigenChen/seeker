<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- 排除敏感数据 -->
        <exclude domain="sharedpref" path="location_tracker.xml"/>
        <exclude domain="database" path="location_tracker.db"/>
    </cloud-backup>
    <device-transfer>
        <!-- 设备转移时也排除敏感数据 -->
        <exclude domain="sharedpref" path="location_tracker.xml"/>
        <exclude domain="database" path="location_tracker.db"/>
    </device-transfer>
</data-extraction-rules>
